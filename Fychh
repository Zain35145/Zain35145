public class Solution
{
    public bool solution(int[] A)
    {
        HashSet<int> set = new HashSet<int>();

        foreach (int num in A)
        {
            if (set.Contains(num + 1) || set.Contains(num - 1))
                return true;

            set.Add(num);
        }

        return false;
    }
}
